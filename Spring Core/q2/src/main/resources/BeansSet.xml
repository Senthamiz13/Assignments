<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <beans>
        <bean id="myquestion" class="SpringSet.Question">
            <property name="questionId" value="124"></property>
            <property name="question" value="Types of pollution?"></property>
            <property name="answers">
                <set>
                    <value>"Air Pollution"</value>
                    <value>"Water Pollution"</value>
                    <value>"Light Pollution"</value>
                    <value>"Soil Pollution"</value>
                </set>
            </property>
        </bean>
    </beans>
</beans>